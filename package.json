  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "build": "nest build"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphile-worker": "^0.16.6",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "keycloak-connect": "^25.0.6",
    "nest-keycloak-connect": "^1.9.5",
    "nest-winston": "^1.9.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "ulid": "^2.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.7",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm-extension": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "testTimeout": 10000,
    "verbose": false,
    "maxWorkers": 1,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^root(.*)$": "<rootDir>/src$1",
      "^shared(.*)$": "<rootDir>/shared$1",
      "^authentication(.*)$": "<rootDir>/contexts/authentication$1",
      "^database(.*)$": "<rootDir>/database$1",
      "^common(.*)$": "<rootDir>/contexts/common$1",
      "^area(.*)$": "<rootDir>/contexts/area$1",
      "^company(.*)$": "<rootDir>/contexts/company$1",
      "^cost-center(.*)$": "<rootDir>/contexts/cost-center$1",
      "^employee(.*)$": "<rootDir>/contexts/employee$1",
      "^organization-unit(.*)$": "<rootDir>/contexts/organization-unit$1",
      "^person(.*)$": "<rootDir>/contexts/person$1",
      "^position(.*)$": "<rootDir>/contexts/position$1",
      "^role(.*)$": "<rootDir>/contexts/role$1",
      "^tree(.*)$": "<rootDir>/contexts/tree$1",
      "^user(.*)$": "<rootDir>/contexts/user$1",
      "^account-manager(.*)$": "<rootDir>/contexts/account-manager$1",
      "^enum(.*)$": "<rootDir>/enums$1",
      "^uuid-resolver(.*)$": "<rootDir>/contexts/uuid-resolver$1",
      "^history(.*)$": "<rootDir>/contexts/history$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}